{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     moveFiles: ['cyber-data/fetch_cyber_data.json','/archive/'],",
				"     documentForm: 'documentPerLine',",
				"     dateFormats: ['dd/MM/yyyy'],",
				"     timestampFormats: ['MM/dd/yyyy HH:mm:ss'],",
				"     preferredIntegralType: 'None',",
				"     preferredFractionalType: 'decimal',",
				"     wildcardPaths:['cyber-threat-data/*.json']) ~> source1"
			]
		}
	}
}